fun ExceptionCode ExceptionType =
  if Nat.<(Cast.ExceptionTypeToNat ExceptionType,12)
    then let
           val x0 = (#Cause) (!CP0)
         in
           CP0 :=
           (CP0_Cause_rupd
              ((!CP0),
               CauseRegister_ExcCode_rupd
                 (x0,
                  case ExceptionType of
                     Int => BitsN.B(0x0,5)
                   | Mod => BitsN.B(0x1,5)
                   | TLBL => BitsN.B(0x2,5)
                   | TLBS => BitsN.B(0x3,5)
                   | AdEL => BitsN.B(0x4,5)
                   | AdES => BitsN.B(0x5,5)
                   | Sys => BitsN.B(0x8,5)
                   | Bp => BitsN.B(0x9,5)
                   | ResI => BitsN.B(0xA,5)
                   | CpU => BitsN.B(0xB,5)
                   | Ov => BitsN.B(0xC,5)
                   | Tr => BitsN.B(0xD,5)
                   | _ => BitsN.B(0x0,5))))
         end
  else ();

fun SignalException ExceptionType =
  ( if (#IE)((#Status) (!CP0))
      then ( if not((#EXL)((#Status) (!CP0)))
               then if Option.isSome (!BranchDelay)
                      then ( CP0 :=
                             (CP0_EPC_rupd
                                ((!CP0),BitsN.-((!PC),BitsN.B(0x4,64))))
                           ; let
                               val x0 = (#Cause) (!CP0)
                             in
                               CP0 :=
                               (CP0_Cause_rupd
                                  ((!CP0),CauseRegister_BD_rupd(x0,true)))
                             end
                           )
                    else ( CP0 := (CP0_EPC_rupd((!CP0),(!PC)))
                         ; let
                             val x0 = (#Cause) (!CP0)
                           in
                             CP0 :=
                             (CP0_Cause_rupd
                                ((!CP0),CauseRegister_BD_rupd(x0,false)))
                           end
                         )
             else ()
           ; let
               val vectorOffset =
                 if (ExceptionType = XTLBRefill) andalso
                    (not((#EXL)((#Status) (!CP0))))
                   then BitsN.B(0x80,30)
                 else BitsN.B(0x180,30)
             in
               ( ExceptionCode ExceptionType
               ; let
                   val x0 = (#Status) (!CP0)
                 in
                   CP0 :=
                   (CP0_Status_rupd
                      ((!CP0),StatusRegister_EXL_rupd(x0,true)))
                 end
               ; let
                   val vectorBase =
                     if (#BEV)((#Status) (!CP0))
                       then BitsN.B(0xFFFFFFFFBFC00200,64)
                     else BitsN.B(0xFFFFFFFF80000000,64)
                 in
                   ( BranchDelay := NONE
                   ; BranchTo := NONE
                   ; PC :=
                     (BitsN.@@
                        (BitsN.bits(vectorBase,63,30),
                         BitsN.+(BitsN.bits(vectorBase,29,0),vectorOffset)))
                   )
                 end
               )
             end
           )
    else ( BranchTo := NONE; PC := (BitsN.+((!PC),BitsN.B(0x4,64))) )
  ; exceptionSignalled := true
  );

